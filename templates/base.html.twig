<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <header>
        <h1><a href="{{ path('homepage') }}">Guestbook</a> </h1>
        <h1><a href="{{ path('admin') }}">Admin panel</a> </h1>
        <ul>
            {% for conference in conferences.onControllerEvent %}
                <li><a href="{{ path('conference', {id: conference.id}) }}">{{ conference }}</a> </li>
            {% endfor %}
        </ul>
        <hr>
    </header>
    <body>
        {% block body %}
        <p>This part came from base.html template</p>
        {% endblock %}
        {% block footer %}
            <p> haha venis</p>
    {% endblock footer %}
    </body>
</html>
